<?xml version="1.0" encoding="utf-8"?>
<Container>
<Beta>False</Beta>
<Category>network:</Category>
  <Name>NPS-Client</Name>
  <Description>
    使用方法：
    三个变量环境：
    目标服务器IP
    目标服务器端口
    目标服务器KEY
    按照顺序填入APPLY 即可使用
  </Description>
  <Overview>内网穿透</Overview>
  <Support>https://www.lxg2016.com/53740.html</Support>
  <Registry>https://hub.docker.com/r/rsg2019/npc-client</Registry>
  <Repository>rsg2019/npc-client</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
    <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>9980</HostPort>
        <ContainerPort>9980</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data/>
  <Environment>
    <Variable>
      <Value>cloud\\.yourdomain\\.com</Value>
      <Name>domain</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>admin</Value>
      <Name>username</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>yourpassword</Value>
      <Name>password</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Icon>http://i.imgur.com/LWfuAG2.png</Icon>
</Container>
