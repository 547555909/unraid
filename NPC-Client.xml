<?xml version="1.0"?>
<Container version="2">
<Beta>False</Beta>
<Category>network:</Category>
  <Name>NPS-Client</Name>
  <Description>
    使用方法：
    三个变量环境： /目标服务器IP  /目标服务器端口  /目标服务器KEY /按照顺序填入APPLY 即可使用
  </Description>
  <Overview>NPS-CLIENT</Overview>
  <Support>https://www.lxg2016.com/53740.html</Support>
  <Registry>https://hub.docker.com/r/rsg2019/npc-client</Registry>
  <Repository>rsg2019/npc-client</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Network>host</Network>
  <Data/>
  <Environment>
    <Variable>
      <Value>IP</Value>
      <Name>服务器的IP</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>PORT</Value>
      <Name>服务器端口</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>VKEY</Value>
      <Name>连接的KEY</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Icon>http://i.imgur.com/LWfuAG2.png</Icon>
</Container>
