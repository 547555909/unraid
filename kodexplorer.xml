<?xml version="1.0"?>
<Container>
  <Name>kodexplorer</Name>
  <Repository>rsg2019/kodexplorer</Repository>
  <Registry>https://hub.docker.com/repository/docker/rsg2019/kodexplorer</Registry>
  <Network>host</Network>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>www.lxg2016.com</Support>
  <Project>https://hub.docker.com/repository/docker/rsg2019/kodexplorer</Project>
  <Overview>
    可道云，目录建议映射/mnt方便管理和读取整个用户区
  </Overview>
  <WebUI>http://[IP]:[PORT:9988]<WebUI/>
  <Environment>
    <Variable>
      <Value>99</Value>
      <Name>PUID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>100</Value>
      <Name>PGID</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/doh-server</HostDir>
      <ContainerDir>/config/www/data/User/admin/home/我的文档/</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
</Container>
