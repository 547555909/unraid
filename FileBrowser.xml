<?xml version="1.0"?>
<Container version="2">
  <Name>FileBrowser</Name>
  <Repository>80x86/filebrowser</Repository>
  <Registry>https://hub.docker.com/r/80x86/filebrowser</Registry>
  <Network>host</Network>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>www.lxg2016.com</Support>
  <Project>https://hub.docker.com/r/80x86/filebrowser</Project>
  <Overview>
    使用荒野无灯大神镜像！具体教请访问www.lxg2016.com
  </Overview>
  <Category>Cloud: Network:Web Status:Stable</Category>
  <WebUI>http://[IP]:[PORT:8083]</WebUI>
  <TemplateURL/>
  <Icon>https://github.com/maschhoff/docker/raw/master/filebrowser/35781395.png</Icon>
  <ExtraParams></ExtraParams>
  <PostArgs></PostArgs>
  <CPUset/>
  <DateInstalled></DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Description></Description>
  <Networking>
    <Mode>host</Mode>
    <Publish/>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/filebrowser</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>想管理的路径</HostDir>
      <ContainerDir>/myfiles</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>1000</Value>
      <Name>PUID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>100</Value>
      <Name>PGID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>on或off</Value>
      <Name>SSL</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>000</Value>
      <Name>UMASK_SET</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>8083</Value>
      <Name>WEB_PORT</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
  <Config Name="Host Path 1" Target="/myfiles" Default="" Mode="rw" Description="Container Path: /myfiles" Type="Path" Display="always" Required="false" Mask="false">/mnt/</Config>
  <Config Name="Host Path 2" Target="/config/" Default="" Mode="rw" Description="Container Path: /config/" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/filebrowser/</Config>
</Container>
